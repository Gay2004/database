postgres=# CREATE TABLE Departments (
    DepartmentID SERIAL PRIMARY KEY,
    Name VARCHAR(100),
    Location VARCHAR(100)
);
CREATE TABLE
postgres=# CREATE TABLE Professors (
    ProfessorID SERIAL PRIMARY KEY,
    Name VARCHAR(100),
    Email VARCHAR(100),
    Office VARCHAR(50),
    DepartmentID INT REFERENCES Departments(DepartmentID)
);
CREATE TABLE
postgres=# CREATE TABLE Courses (
    CourseID SERIAL PRIMARY KEY,
    Title VARCHAR(100),
    Credits INT,
    DepartmentID INT REFERENCES Departments(DepartmentID),
    ProfessorID INT REFERENCES Professors(ProfessorID)
);
CREATE TABLE
postgres=# CREATE TABLE Enrollments (
    StudentID INT REFERENCES Students(StudentID),
    CourseID INT REFERENCES Courses(CourseID),
    PRIMARY KEY (StudentID, CourseID)
);
